{
  "tasks": {
    "cli": "deno run --allow-net --allow-read --allow-write --allow-env ./cli/cli.ts"
  },
  "name": "@elwood/db",
  "version": "0.0.11",
  "imports": {
    "@/": "./src/",
    "kysely": "npm:kysely@^0.27.4"
  },
  "exports": {
    ".": "./mod.ts",
    "./lib": "./src/lib/mod.ts",
    "./types": "./src/types.ts",
    "./constants": "./src/constants.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "readme.md",
      "LICENSE",
      "mod.ts"
    ],
    "exclude": [
      "src/cli.ts"
    ]
  }
}